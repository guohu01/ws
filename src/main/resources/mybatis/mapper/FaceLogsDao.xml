<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.yoyo.ws.dao.FaceLogsDao">
    <select id="getByIdAndDate" resultType="cn.yoyo.ws.model.FaceLogs">
        select * from t_facelogs where user_id=#{id} and recog_time=#{enterDate}
    </select>
</mapper>